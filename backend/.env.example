# OpenAI API Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Qdrant Cloud Configuration
QDRANT_URL=https://your-instance.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key-here

# Neon Postgres Configuration
DATABASE_URL=postgresql://user:password@ep-name.neon.tech/dbname

# Admin Token
ADMIN_TOKEN=your-secure-admin-token-here

# Backend Configuration
PORT=8000
ENVIRONMENT=development
DEBUG=false

# Logging
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_QUERIES_PER_HOUR=100

# CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000,http://localhost:4000
